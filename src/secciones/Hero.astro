---
interface Props{
    tituloSeccion: string;
    subtituloSeccion: string ;
}

const {tituloSeccion, subtituloSeccion} = Astro.props
---


<section>
    <h1>{tituloSeccion}</h1>
    <p>{subtituloSeccion}</p>
</section>

<style>
    section{
        width: 100%;
        height: 60vh;
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        text-align: center;
        gap: 40px;
    }
</style>